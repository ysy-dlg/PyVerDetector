{
    "name": "Python version detector",
    "description": "Python version detector for Python code snippets on Stack Overflow.",
    "version": "1.0",
    "manifest_version": 3,

    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["scripts/pycomply.js", "scripts/content.js"],
        "matches": ["https://stackoverflow.com/*"]
    }],

    "web_accessible_resources": [{
      "resources": ["scripts/pycomply.wasm"],
      "matches": ["https://stackoverflow.com/*"]
    }],

    "icons": { 
        "16": "imgs/icon.png",
        "48": "imgs/icon.png",
        "128": "imgs/icon.png" 
    }
}
